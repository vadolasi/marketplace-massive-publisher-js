{
  "name": "marketplace-massive-publisher",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "sucrase-node src/index.ts",
    "transpile": "npx tsc",
    "obfuscate": "javascript-obfuscator ./dist/js --output ./dist/obfuscated",
    "build": "pkg .",
    "deploy": "yarn transpile && yarn obfuscate && yarn build"
  },
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "javascript-obfuscator": "^3.0.0",
    "pkg": "^5.3.3",
    "sucrase": "^3.20.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "inquirer": "^8.2.0",
    "inquirer-fs-selector": "^1.5.0",
    "playwright": "^1.16.1"
  },
  "bin": "dist/obfuscated/index.js",
  "pkg": {
    "scripts": "dist/obfuscated/**/*.js",
    "outputPath": "dist/build"
  }
}
